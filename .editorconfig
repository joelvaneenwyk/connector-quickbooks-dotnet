# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
line_length = 130

printWidth = 130

[*.{yml,yaml}]
indent_size = 2

[*.{vcxproj,csproj,Build.props,Build.targets}]
indent_size = 2
